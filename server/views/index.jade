doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, minimum-scale=1.0, maximum-scale=1.0, initial-scale=1.0")
		title Webinate

		link(rel="stylesheet",type="text/css",href=url + "/admin/style.css")
		link(rel="stylesheet",type="text/css",href=url + "/admin/loading-bar.css")
		link(href="https://fonts.googleapis.com/css?family=Lily+Script+One", rel="stylesheet", type="text/css")
		link(href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300italic,300", rel="stylesheet", type="text/css")
		script(type="text/javascript").
			var _users = "#{usersURL}"
			var _cache = "#{cacheURL}"
			var _plugins = [];
			#{scriptOptions}
		script(type="text/javascript", src=url + "/admin/js-libs/jquery-2.1.3.min.js")
		script(type="text/javascript", src=url + "/admin/js-libs/angular.min.js")
		script(type="text/javascript", src=url + "/admin/js-libs/angular-ui-router.min.js")
		script(type="text/javascript", src=url + "/admin/js-libs/angular-animate.min.js")
		script(type="text/javascript", src=url + "/admin/js-libs/angular-sanitize.min.js")
		script(type="text/javascript", src=url + "/admin/js-libs/ng-file-upload.min.js")
		script(type="text/javascript", src=url + "/admin/js-libs/loading-bar.min.js")
		script(type="text/javascript", src="https://www.google.com/recaptcha/api/js/recaptcha_ajax.js")
		script(type="text/javascript", src=url + "/admin/js-libs/tinymce/tinymce.min.js")
		each plugin, index in plugins
			script(type="text/javascript", src=url + plugin)
		script(type="text/javascript", src=url + "/admin/main.js")
		base(href="/")

	body(ng-app="admin")
		div(class="main-view", ui-view="main-view")